@model SilveR.StatsModels.OneWayANOVAPowerAnalysisUserBasedInputsModel

@section Scripts {
    <script>
        var theModel = @Html.Raw(Json.Serialize(Model));
    </script>

    <environment names="Development">
        <script src="~/js/stats/oneWayANOVAPowerAnalysisUserBasedInputs.js"></script>
    </environment>
    <environment names="Production">
        <script src="~/js/stats/oneWayANOVAPowerAnalysisUserBasedInputs.min.js"></script>
    </environment>
}

@{
    ViewBag.Title = "'One-way ANOVA' Power Analysis (User Based Inputs)";
}

<h2>@ViewBag.Title</h2>

<form action="/Analyses/OneWayANOVAPowerAnalysisUserBasedInputs" class="form-horizontal" method="post">

    @await Html.PartialAsync("AnalysisErrors")

    <div class="row">
        <div class="col-sm-6">

            <div class="panel panel-default">
                <div class="panel-heading">Input Options</div>
                <div class="panel-body">
                    <div class="col-sm-12">

                        <h4>Group means</h4>
                        <div class="form-group">
                              <div class="col-sm-8">
                                <input class="form-control" asp-for="Means" />
                                <div><span asp-validation-for="Means"></span></div>
                            </div>
                        </div>
                        <br />

                        <h4>Variability estimate</h4>
                        <div class="form-group">
                            <div class="radio">
                                <label>
                                    <input asp-for="VariabilityEstimate" type="radio" value="Variance" onclick="enableDisableVariabilityEstimate()" />
                                    <label>Use Variance</label>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label">Variance</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="Variance" />
                                <div><span asp-validation-for="Variance"></span></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="radio">
                                <label>
                                    <input asp-for="VariabilityEstimate" type="radio" value="StandardDeviation" onclick="enableDisableVariabilityEstimate()" />
                                    <label>Use Standard deviation</label>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label">Standard deviation</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="StandardDeviation" />
                                <div><span asp-validation-for="StandardDeviation"></span></div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-4 control-label">Significance level</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="Significance" />
                                <div><span asp-validation-for="Significance"></span></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-sm-6">

            <div class="panel panel-default">
                <div class="panel-heading">Output Options</div>
                <div class="panel-body">

                    <h4>Plotting range</h4>
                    <div class="col-sm-12">
                        <div class="form-group">
                            <div class="radio">
                                <label>
                                    <input asp-for="PlottingRangeType" type="radio" value="SampleSize" onclick="enableDisablePlottingRangeType()" />
                                    <label>Use Sample Size</label>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label">Sample size from</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="SampleSizeFrom" />
                                <div><span asp-validation-for="SampleSizeFrom"></span></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label" for="SampleSizeTo">Sample size to</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="SampleSizeTo" />
                                <div><span asp-validation-for="SampleSizeTo"></span></div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="radio">
                                <label>
                                    <input asp-for="PlottingRangeType" type="radio" value="Power" onclick="enableDisablePlottingRangeType()" />
                                    <label>Use Power</label>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label">Power from</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="PowerFrom" />
                                <div><span asp-validation-for="PowerFrom"></span></div>
                            </div>

                        </div>
                        <div class="form-group">
                            <label class="col-sm-4 control-label">Power to</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="PowerTo" />
                                <div><span asp-validation-for="PowerTo"></span></div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-4 control-label">Graph title</label>
                            <div class="col-sm-8">
                                <input class="form-control" asp-for="GraphTitle" />
                                <div><span asp-validation-for="GraphTitle"></span></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row mt20">
        <div class="col-md-12">
            <button class="btn btn-primary btn-lg" type="submit">Submit</button>
        </div>
    </div>
</form>